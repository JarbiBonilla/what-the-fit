# form for creating a new trainer profile

<h1> Sign Up Here! </h1>

<%= form_for @trainer do |f| %>
    <%= f.label :name %>
    <%= f.text_field :name %>
    <br>
    <br>
    <%= f.label :email %>
    <%= f.text_field :email %>
    <br>
    <br>
    <%= f.label :password %>
    <%= f.password_field :password %>
    <br> 
    <br>
    <%= f.label :bio %>
    <%= f.text_area :bio %>
    <br>
    <br>

    <%= f.submit %>

    <% end %>